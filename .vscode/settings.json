{
  "python.languageServer": "Pylance",
  "tailwindCSS.includeLanguages": {
      "html": "html",
      "javascript": "javascript",
      "css": "css"
  },
  "editor.quickSuggestions": {
      "strings": true
  },
  "files.associations": {
      "**/*.html": "html",
      "**/templates/**/*.html": "django-html",
      "**/templates/**/*": "django-txt",
      "**/requirements{**,*}.{txt,in}": "pip-requirements",
  }
}